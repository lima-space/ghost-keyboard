{
  "name": "ghost-keyboard",
  "version": "1.0.7",
  "description": "Simulate any keyboard behaviour even not installed",
  "main": "dist/ghost-keyboard.js",
  "scripts": {
    "start": "webpack --watch",
    "dev": "concurrently \"webpack --progress --colors --watch\" \"karma start --colors\"",
    "test": "karma start --single-run --browsers ChromeHeadlessNoSandbox",
    "build": "NODE_ENV=production webpack --config webpack.config.js"
  },
  "author": {
    "name": "Felipe Lima",
    "url": "https://github.com/fill-lima/ghost-keyboard"
  },
  "keywords": [
    "keyboard",
    "virtual"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fill-lima/ghost-keyboard.git"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "ts-loader": "^9.4.2",
    "typescript": "^3.3.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
